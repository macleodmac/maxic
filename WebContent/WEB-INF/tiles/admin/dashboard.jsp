<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<div class="row">
	<div class="col-xs-4 text-right">
		<div class="form-group">
			<p class="form-control-static">Search Towers:</p>
		</div>
	</div>
	<div class="col-xs-8">
		<form class="form-inline">
			<div class="form-group">
				<input type="text" class="form-control input-block-level"
					id="towerSearch" placeholder="Enter search term..." />
			</div>
			<button type="submit" class="btn btn-default">Submit</button>
		</form>
	</div>
</div>
<div class="row">
	<div class="col-xs-4 text-right">
		<div class="form-group">
			<p class="form-control-static">Search Peals:</p>
		</div>
	</div>
	<div class="col-xs-8">
		<form class="form-inline">
			<div class="form-group">
				<input type="text" class="form-control input-block-level"
					id="pealSearch" placeholder="Enter search term..." />
			</div>

			<button type="submit" class="btn btn-default">Submit</button>
		</form>
	</div>
</div>
<div class="row">
	<div class="col-xs-4 text-right">
		<div class="form-group">
			<p class="form-control-static">Search Users:</p>
		</div>
	</div>
	<div class="col-xs-8">
		<form class="form-inline">
			<div class="form-group">
				<input type="text" class="form-control input-block-level"
					id="userSearch" placeholder="Enter search term..." />
			</div>

			<button type="submit" class="btn btn-default">Submit</button>
		</form>
	</div>
</div>
